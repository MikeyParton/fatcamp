<% if @user == current_user %>
	<div class="panel panel-default">
	    <div class="panel-heading">
	    	<p class="panel-title">
					People you may know:
				</p>
	    </div>
	    <ul class="list-group">

				<% @friend_suggestions.each do |other_user| %>
					<% if other_user.id != @user.id %>
						<li id=id="user-<%=other_user.id%>" class="list-group-item">
				    	<%= render partial: '/users/user_item', locals: { other_user: other_user } %>
				    </li>
		    	<% end %>
		    <% end %>
	    </ul>
	</div>
<% end %>